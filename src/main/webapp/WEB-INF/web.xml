  	<servlet-class>character.CalculateScores</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>Controller</servlet-name>
  	<url-pattern>/Controller</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>CalculateScores</servlet-name>
  	<url-pattern>/CalculateScores</url-pattern>
  </servlet-mapping>
