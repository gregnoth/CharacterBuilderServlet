<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
  	<servlet-name>Controller</servlet-name>
  	<display-name>Controller</display-name>
  	<description></description>
  	<servlet-class>character.Controller</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>CalculateScores</servlet-name>
  	<display-name>CalculateScores</display-name>
